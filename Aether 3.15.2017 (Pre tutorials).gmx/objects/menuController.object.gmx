            <string>///Check Over Button and Button Events

//Set Button Count
buttonCount = array_height_2d(button);

//Check for Each Button, whether or not the mouse is over the button
for(i = 0; i &lt; buttonCount; i++) {
    var xx = abs(button[i, 1]+buttonWidth/2 - mouse_x);
    var yy = abs(button[i, 2]+buttonHeight/2 - mouse_y);
    overButton[i] = (xx &lt; buttonWidth/2) &amp;&amp; (yy &lt; buttonHeight/2);

    //Check Whether the Mouse is over a button
    if(overButton[i]) {
        //If so check for a Mouse Left Click
        if(mouse_check_button_released(mb_left)) {
            //Check Which Button was clicked
            switch(button[i, 0]) {
                case "Play":
                    room_goto(room0);
                break;
                
                case "Options":
                    defunct = true;
                    alarm[0] = 1.25*room_speed;
                break;
                
                case "Quit":
                    game_end();
                break;
            }
        }
    }
}
